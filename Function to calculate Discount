import java.util.*;
public class Discount
{
 public static void main(String args[])
 {
     Scanner sc=new Scanner(System.in);
     double a=sc.nextDouble();
     double b=sc.nextDouble();
  double dis,amount;
  
        dis=100-b;
 
	amount= (dis*a)/100;
 
	System.out.printf("%.1f",amount);
 
	}
}
