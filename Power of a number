//Code here
import java.util.Scanner;
public class Powerofanumber
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        
        int n=sc.nextInt();
        int m=sc.nextInt();
        double e=Math.pow(n,m);
        System.out.printf("%.0f",e);
    }
}
